{
  "name": "Shortcuts for Trello",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Adds more extensions to Trello",
  "homepage_url": "https://github.com/bulkan/shortcuts-for-trello",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "permissions": [
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "js": [
        "src/inject/inject.js",
        "vendor/jquery-2.1.1.min.js"
      ]
    }
  ]
}
